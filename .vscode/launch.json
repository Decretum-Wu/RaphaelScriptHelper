    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python Debugger: Current File",
            "type": "debugpy",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal"
        },
        {
            "name": "Python Debugger: Capture",
            "type": "debugpy",
            "request": "launch",
            "program": "./CaptureMarkHelper.py",
            "console": "integratedTerminal"
        },
        {
            "name": "Python Debugger: debug",
            "type": "debugpy",
            "request": "launch",
            "program": "./debug.py",
            "console": "integratedTerminal"
        }
    ]
}